        function submitProdouct() {


            var form = document.querySelector(".formFrame")
            console.log(form)
            var data = new FormData(form);
            $.ajax({
                url: "/RequiredPost/commemtProductPost",
                type: "Post",
                data: data,
                processData: false,
                contentType: false,
                success: function (response) {
                    window.alert(response)
                    window.location.href = "/PersonalMarket/Personal_Index"

                }
            })
        }//end  of submit product


        function tag() {
            var txt = ""
            $.ajax({
                url: "/RequiredPost/getAllTag",
                type: "GET",
                success: function (response) {
                    $(response).each(function (i, item) {
                        // txt += <a>itrm.tagid </a>
                        txt += `<option value=${item.TagID}>${item.tagName}</option>`
                        $("h6>select").html(txt)
                    })
                }
            })

        }// end tag function

        function previewFile() {
            //var photo = $(eler).val();
            const preview = document.querySelector('#Productimg');
            const file = document.querySelector('input[type=file]').files[0];
            const reader = new FileReader();

            reader.addEventListener("load", function () {
                // convert image file to base64 string
                preview.src = reader.result;
            }, false);

            if (file) {
                reader.readAsDataURL(file);
            }
        } //  doUpload end

